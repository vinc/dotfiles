[alias]
  a  = add
  b  = branch
  c  = commit
  co = checkout
  d  = diff
  l  = log
  ll = log --oneline
  lg = log --oneline --graph
  s  = status
[color]
  ui = true
[core]
  autocrlf = input
  excludesfile = ~/.gitignore
[user]
  name = <%= $stdout.write('Your Name: '); $stdin.gets.chomp %>
  email = <%= $stdout.write('Your Email: '); $stdin.gets.chomp %>
[github]
  user = <%= $stdout.write('Your GitHub Username: '); $stdin.gets.chomp %>
